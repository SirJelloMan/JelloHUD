"UnlitGeneric"
{
	"$basetexture" "passtime/hud/passtime_goal_blue_unlocked"
	"$translucent" 1
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$ignorez" 1
	"$nofog" 1
	"$nocull" 1

	"$spriteorientation" 4

"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 20
		}

	}


}
